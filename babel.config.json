{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "ignore": [
    "node_modules",
    "node_modules/",
    "node_modules/*",
    "./node_modules",
    "./node_modules/",
    "./node_modules/*",
    "./**/node_modules",
    "./**/node_modules/",
    "./**/node_modules/*"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    ["@babel/plugin-transform-runtime"]
  ]
}
