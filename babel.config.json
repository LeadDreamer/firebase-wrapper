{
  "sourceType": "module",
  "presets": ["@babel/preset-env"],
  "ignore": [
    "node_modules",
    "node_modules/",
    "node_modules/*",
    "./node_modules",
    "./node_modules/",
    "./node_modules/*",
    "./**/node_modules",
    "./**/node_modules/",
    "./**/node_modules/*"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    ["@babel/plugin-proposal-private-property-in-object", { "loose": true }],
    ["@babel/plugin-proposal-private-methods", { "loose": true }],
    ["@babel/plugin-transform-runtime"]
  ]
}
